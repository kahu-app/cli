{
  "name": "kahu/cli",
  "description": "Kahu.app Command-line interface",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Flavio Heleno",
      "email": "flaviohbatista@gmail.com",
      "homepage": "https://flavioheleno.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Kahu\\Cli\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist",
    "allow-plugins": {
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "path",
      "url": "../oauth2-client",
       "options": {
          "symlink": true,
          "versions": {
            "kahu/oauth2-client": "dev-main"
          }
      }
    }
  ],
  "require": {
    "php": "^8.2",
    "ext-json": "*",
    "ext-pcntl": "*",
    "amphp/dns": "^2.0",
    "amphp/http-server": "v3.0.0-beta.8",
    "flavioheleno/jay": "^0.3.0",
    "guzzlehttp/guzzle": "^7.6",
    "kahu/oauth2-client": "dev-main",
    "laravel-zero/phar-updater": "^1.3",
    "league/config": "^1.2",
    "league/oauth2-client": "^2.7",
    "php-di/php-di": "^7.0",
    "psr/container": "^2.0",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-message": "^1.1",
    "psr/log": "^3.0",
    "symfony/console": "^6.2",
    "symfony/filesystem": "^6.2",
    "symfony/process": "^6.2",
    "teapot/status-code": "^2.1"
  },
  "require-dev": {
    "psy/psysh": "^0.11.17"
  }
}
